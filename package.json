    "lint": "eslint --cache 'src/**/*.{ts,tsx,jsx}' && stylelint '**/**.tsx' && tsc && ts-unused-exports tsconfig.json",
    "start": "react-scripts start",
    "test": "react-scripts test && npm run lint"
